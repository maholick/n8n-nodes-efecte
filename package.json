{
  "name": "n8n-nodes-efecte",
  "version": "0.1.0",
  "description": "Efecte nodes for n8n",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://maholick.com",
  "author": {
    "name": "Shawn Maholick",
    "email": "shawn@maholick.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maholick/n8n-nodes-efecte"
  },
  "engines": {
    "node": ">=20.19",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "n8n-node build",
    "dev": "n8n-node dev",
    "format": "prettier nodes credentials --write",
    "lint": "n8n-node lint",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/EfecteEsmApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/EfecteEsm/EfecteEsm.node.js",
      "dist/nodes/EfecteEsm/EfecteEsmTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.17.0",
    "@types/node": "^20.19.27",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "n8n-workflow": "*",
    "prettier": "^3.4.0",
    "typescript": "^5.6.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "pnpm": {
    "overrides": {
      "axios": "^1.12.0",
      "form-data": "^4.0.4"
    }
  }
}
